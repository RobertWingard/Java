package com.robert.zookeeper;

public class Mammal {
	private int displayEnergy;
	private String eatBananas;
	private String climb;
	private String throwSomething;
	
	
	
	
	
	// Method
	public double displayEnergy() {
		System.out.println("This is the Energy: " +  this.getDisplayEnergy());
		return this.getDisplayEnergy();		
	}
	
	// constructor
	
	public  Mammal() {}
	
	
	public Mammal(int energy) {
		this.setDisplayEnergy(energy);
	}
	
	

	
	//Getters and Setters
	public double DisplayEnergy() {
		return getDisplayEnergy();
		
	}
	public void setDisplayEnergy(int displayEnergy) {
		this.displayEnergy = displayEnergy;
	}
	public String getEatBananas() {
		return eatBananas;
	}
	public void setEatBananas(String eatBananas) {
		this.eatBananas = eatBananas;
	}
	public String getClimb() {
		return climb;
	}
	public void setClimb(String climb) {
		this.climb = climb;
	}
	public String getThrowSomething() {
		return throwSomething;
	}
	public void setThrowSomething(String throwSomething) {
		this.throwSomething = throwSomething;
	}

	public int getDisplayEnergy() {
		return displayEnergy;
	}
}
