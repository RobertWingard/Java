package com.robert.zookeeper;

public class ZooKeeperApplication {

	public static void main(String[] args) {
		Mammal ZooKeeper = new Mammal(100);
		Gorilla Monkey1 = new Gorilla(10);
			ZooKeeper.displayEnergy();
			ZooKeeper.getEatBananas();
			ZooKeeper.getClimb();
			ZooKeeper.getThrowSomething();
			Monkey1.displayEnergy();
			Monkey1.getThrowSomething();
			

	}

}
